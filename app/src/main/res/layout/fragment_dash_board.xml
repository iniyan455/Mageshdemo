    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:gravity="center"
        android:layout_marginBottom="60dp">


        <Button
            android:id="@+id/button"
            android:text="@string/btn_name"
            android:textColor="#626262"
            android:textSize="20sp"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/buttonshape"
            android:shadowColor="#A8A8A8"
            android:fontFamily="@font/arial_regular"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="50dp"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="0" />

        <Button
            android:layout_below="@+id/button"
            android:id="@+id/incompletebtn"
            android:text="@string/incomplete_orders"
            android:textColor="#626262"
            android:textSize="20sp"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/buttonshape"
            android:shadowColor="#A8A8A8"
            android:fontFamily="@font/arial_regular"
            android:layout_marginTop="30dp"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="0" />

        <Button
            android:layout_below="@+id/incompletebtn"
            android:id="@+id/completebtn"
            android:text="complete orders"
            android:textColor="#626262"
            android:textSize="20sp"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/buttonshape"
            android:shadowColor="#A8A8A8"
            android:fontFamily="@font/arial_regular"
            android:layout_marginTop="30dp"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="0" />


        <Button
            android:layout_below="@+id/completebtn"
            android:id="@+id/sentbtn"
            android:text="sent orders"
            android:textColor="#626262"
            android:textSize="20sp"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/buttonshape"
            android:shadowColor="#A8A8A8"
            android:fontFamily="@font/arial_regular"
            android:layout_marginTop="30dp"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="0" />




